{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      },
      {
        "src": "back/public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back/public/index.php"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
}